{
    "name": "Turbot",
    "manifest_version": 3,
    "version": "1.0",
  "icons": {
    "48": "turbo.png"
  },
    "permissions": [
      "storage",
      "tabs"
    ],
    "action": {
      "default_popup": "popup.html"

    },
    "host_permissions": [
      "https://*/*",
      "http://*/*"
    ],
    "content_scripts": [
      {
        "matches": ["https://*/*", "http://*/*"],
        "js": ["content.js"]
      }
    ]
  }
  