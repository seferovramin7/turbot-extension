{
  "name": "Turbot",
  "manifest_version": 3,
  "version": "1.2",
  "icons": {
    "48": "turbo.png"
  },
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"

  },
  "host_permissions": [
    "https://turbo.az",
    "https://codengineers.dev"
  ],
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": ["carFunctionality.js", "searchFunctionality.js"]
    }
  ]
}