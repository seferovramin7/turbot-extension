{
    "name": "Turbot",
    "manifest_version": 3,
    "version": "1.0",
    "permissions": [
      "storage"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": [
      "https://*/*",
      "http://*/*"
    ],
    "content_scripts": [
      {
        "matches": ["https://*/*", "http://*/*"],
        "js": ["content.js"]
      }
    ]
  }
  